@model IEnumerable<R3MUS.Devpack.Recruitment.Models.CorporationModel>

<div class="col-md-6">
	<h3 class="option-selector" onclick="slideToggle(this)">Employment History</h3>
	<div style="display:none;">
		<table class="table">
			<tr>
				<th>
					@Html.DisplayNameFor(model => model.Name)
				</th>
				<th>
					@Html.DisplayNameFor(model => model.StartDate)
				</th>
				<th></th>
			</tr>

			@foreach (var item in Model)
			{
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.Name)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.StartDate)
					</td>
				</tr>
			}
		</table>
	</div>
</div>